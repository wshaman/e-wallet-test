paths:
    migrations: %%PHINX_CONFIG_DIR%%/../db/migrations
    seeds: %%PHINX_CONFIG_DIR%%/../db/seeds

environments:
    default_migration_table: phinxlog
    default_database: common
    common:
        adapter: pgsql
        host: localhost
        name: ol3api
        user: ol3
        pass: ol3-pw
        port: 5432
        charset: utf8
